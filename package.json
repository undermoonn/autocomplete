{
  "name": "vue3-fancy-autocomplete",
  "version": "0.0.10",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node ./scripts/afterBuild.js",
    "serve": "vite preview",
    "test": "yarn jest",
    "format": "prettier --write --parser typescript \"src/packages/**/*.ts?(x)\"",
    "prepare": "husky install",
    "release": "npm publish --access public",
    "pre": "node ./scripts/preRelease.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push --follow-tags"
  },
  "lint-staged": {
    "*.{js,css,scss,md}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/test-utils": "^2.0.0-rc.4",
    "babel-jest": "^26.6.3",
    "babel-node": "^0.0.1-security",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "execa": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.8",
    "semver": "^7.3.5",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "vite": "^2.1.5",
    "vite-plugin-windicss": "^0.12.2",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.0.18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undermoonn/autocomplete.git"
  },
  "keywords": [
    "vue3"
  ],
  "homepage": "https://github.com/undermoonn/autocomplete#readme"
}