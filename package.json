{
  "name": "autocomplete",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "yarn jest",
    "format": "prettier --write --parser typescript \"src/packages/**/*.ts?(x)\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,css,scss,md}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "vue": "^3.0.11"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.3",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/test-utils": "^2.0.0-rc.4",
    "babel-jest": "^26.6.3",
    "babel-node": "^0.0.1-security",
    "chalk": "^4.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.2.3",
    "vite": "^2.1.5",
    "vite-plugin-windicss": "^0.12.2",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.0.18"
  }
}
